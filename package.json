{
    "name": "starter-kit",
    "version": "0.0.1",
    "description": "This is a frontend web developer starter kit",
    "keywords": [
        "html",
        "css",
        "javascript",
        "web development",
        "starter kit",
        "frontend"
    ],
    "author": "Nick Garcia <me@nickgarcia.me> (https://nickgarcia.me)",
    "homepage": "https://nickgarcia.me",
    "main": "./src/index.js",
    "license": "ISC",
    "private": true,
    "scripts": {
        "prestart": "babel-node buildScripts/startMessage.js",
        "start": "npm-run-all --parallel open:src lint:watch test:watch",
        "open:src": "babel-node buildScripts/srcServer.js",
        "lint": "esw webpack.config.* buildScripts src --color",
        "lint:watch": "npm run lint -- --watch",
        "localtunnel": "lt --port 8080",
        "share": "npm-run-all --parallel open:src localtunnel",
        "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
        "test:watch": "npm run test -- --watch",
        "webpack": "webpack"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/NicksIdeaEngine/starter-kit"
    },
    "bugs": {
        "url": "https://github.com/NicksIdeaEngine/starter-kit/issues",
        "email": "me@nickgarcia.me"
    },
    "dependencies": {
        "bootstrap": "^4.3.1",
        "chalk": "^2.4.2",
        "express": "^4.17.1",
        "jquery": "^3.4.1",
        "open": "^7.0.0",
        "popper.js": "^1.15.0",
        "webpack": "^4.41.2",
        "webpack-dev-middleware": "^3.7.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.6.4",
        "@babel/core": "^7.6.4",
        "@babel/node": "^7.6.3",
        "@babel/preset-env": "^7.6.3",
        "@babel/register": "^7.6.2",
        "babel-eslint": "^10.0.3",
        "babel-loader": "^8.0.6",
        "chai": "^4.2.0",
        "css-loader": "^3.2.0",
        "eslint": "^6.5.1",
        "eslint-loader": "^3.0.2",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-watch": "^6.0.1",
        "jsdom": "^15.2.0",
        "localtunnel": "^2.0.0",
        "mocha": "^6.2.2",
        "npm-run-all": "^4.1.5",
        "style-loader": "^1.0.0"
    }
}
